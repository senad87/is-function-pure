    "scripts": {
      "web": "node --max-old-space-size=8192 app/server/index.js",
      "start": "npm run start:server & npm run start:frontend",
      "start:api": "node --max-old-space-size=8192 app/api/index.js",
      "start:api:prod": "nohup npm run start:api > ./api.log &",
      "start:server": "node --max-old-space-size=8192 app/server/index.js",
      "start:frontend": "node ./frontend/server.js",
      "start:prod": "nohup npm start > ./finder.log &",
      "start:mongo": "nohup mongod > mongo.log &",
      "test": "mocha tests --recursive --watch",
      "coverage": "nyc mocha tests --recursive",
      "html-coverage": "nyc report --reporter=html"
    },
